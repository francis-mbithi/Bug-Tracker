{{#partial "content"}}
    <h1 class="text-center">Bugs tracked</h1>
    <div class="container">
        <div class="row">
            <div class="col-md-9">
                {{#each bugs}}
                <a href="/bugs/{{id}}">
                    <div class="card p-4 mt-3">
                    <h3>Language: {{category}}</h3>
                    <p>{{content}}</p>
                </div>
                </a>
                {{/each}}
            </div>
        </div>
    </div>
{{/partial}}

{{> layout.hbs}}